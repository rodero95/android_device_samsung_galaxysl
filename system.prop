#
# system.prop for latona
#

rild.libpath=/system/lib/libsec-ril.so
rild.libargs=-d /dev/ttyS0
ro.sf.lcd_density=240
# Disable zram
ro.zram.default=0


# Disable the preload of the EGL drivers in Zygote at boot time
ro.zygote.disable_gl_preload=true

# Set Galaxy SL release
ro.galaxysl.release=Development
